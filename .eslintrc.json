{
  "env": {
    "browser": true,
    "es2021": true,
    "webextensions": true
  },
  "extends": ["eslint:recommended", "plugin:prettier/recommended"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "no-useless-escape": "off",
    "no-undef": "off"
  }
}